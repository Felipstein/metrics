{
  "editor.rulers": [
    120
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll": "always",
    "source.fixAll.eslint": "always",
    "source.addMissingImports": "always",
    "source.sortMembers": "always"
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma",
    "editor.formatOnSave": true
  },
  "cSpell.words": [
    "clsx",
    "tinycolor"
  ]
}
